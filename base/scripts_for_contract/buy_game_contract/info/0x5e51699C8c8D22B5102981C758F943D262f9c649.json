n        // // Retrieve information from the deployed contract\n        string memory information = deployedContract.getMetadataFromID(gameId);\n\n        // // Emit an event with the retrieved information\n        emit InformationRetrieved(information);\n\n        return information;\n    }\n\n    function updateDeployedContractAddress(\n        address _deployedContractAddress\n    ) external onlyOwner {\n        deployedContractAddress = _deployedContractAddress;\n        deployedContract = DeveloperContract(deployedContractAddress);\n    }\n}\n", "sourceMap": "93:1490:0:-:0;;;313:220;;;;;;;;;;;;;;;;;;;;;;;;;;;;:::i;:::-;385:10;369:26;;405:23;:50;;-1:-1:-1;;;;;405:50:0;;;;-1:-1:-1;;;;;;405:50:0;;;;;;;;;465:61;;;;;;;;93:1490;;14:290:12;84:6;137:2;125:9;116:7;112:23;108:32;105:52;;;153:1;150;143:12;105:52;179:16;;-1:-1:-1;;;;;224:31:12;;214:42;;204:70;;270:1;267;260:12;204:70;293:5;14:290;-1:-1:-1;;;14:290:12:o;:::-;93:1490:0;;;;;;;;;;;;;;;;;", "sourcePath": "contracts/BuyGameContract.sol", "deployment": {"address": "0x5e51699C8c8D22B5102981C758F943D262f9c649", "chainid": "11155111", "blockHeight": 3622576}}